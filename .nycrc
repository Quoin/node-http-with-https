{
  "all": true,
  "report-dir": "./reports/coverage/default",
  "temp-dir": "./reports/nyc_output",
  "check-coverage": false,
  "exclude": [
    "**/*.*.test.js",
    "reports",
    "*.test.js",
    "example",
    "server.js"
  ],
  "reporter": [
    "cobertura",
    "html",
    "text",
    "text-summary"
  ],
  "watermarks": {
    "statements": [50, 80],
    "lines": [50, 80],
    "functions": [50, 80],
    "branches": [50, 80]
  }
}
